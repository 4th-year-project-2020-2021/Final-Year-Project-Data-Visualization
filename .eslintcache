ata.map(d => d.Year))\r\n          .range([0,WIDTH])  \r\n          .padding(0.4)\r\n\r\n      const xAxisCall = d3.axisBottom(x)\r\n        vis.xAxisGroup.transition().duration(500).call(xAxisCall)\r\n\r\n      const yAxisCall = d3.axisLeft(y)\r\n       vis.yAxisGroup.transition().duration(500).call(yAxisCall)\r\n\r\n      // DATA JOIN\r\n      const rects = vis.svg.selectAll(\"rect\")\r\n            .data(vis.data)\r\n\r\n      // EXIT\r\n      rects.exit()\r\n        .transition().duration(500)\r\n          .attr(\"height\", 0)\r\n          .attr(\"y\", HEIGHT)\r\n          .remove()\r\n\r\n      // UPDATE\r\n      rects.transition().duration(500)\r\n        .attr(\"x\", d => x(d.Year))\r\n        .attr(\"y\", d => y(d.Number))\r\n        .attr(\"width\", x.bandwidth)\r\n        .attr(\"height\", d => HEIGHT-y(d.Number))\r\n\r\n\r\n      // ENTER\r\n      rects.enter().append(\"rect\")\r\n        .attr(\"x\", d => x(d.Year))\r\n        .attr(\"width\", x.bandwidth)\r\n        .attr(\"fill\", \"green\")\r\n        .attr(\"y\",HEIGHT)\r\n        .transition().duration(500)\r\n        .attr(\"height\", d => HEIGHT-y(d.Number))\r\n        .attr(\"y\", d => y(d.Number))\r\n    }\r\n}",["80","81"],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":1,"column":1,"nodeType":"88","endLine":1,"endColumn":31},{"ruleId":"86","severity":1,"message":"87","line":1,"column":1,"nodeType":"88","endLine":1,"endColumn":27},{"ruleId":"86","severity":1,"message":"87","line":1,"column":1,"nodeType":"88","endLine":1,"endColumn":29},{"ruleId":"89","severity":1,"message":"90","line":82,"column":9,"nodeType":"91","messageId":"92","endLine":82,"endColumn":16},{"ruleId":"89","severity":1,"message":"93","line":2,"column":10,"nodeType":"91","messageId":"92","endLine":2,"endColumn":13},{"ruleId":"89","severity":1,"message":"93","line":2,"column":10,"nodeType":"91","messageId":"92","endLine":2,"endColumn":13},{"ruleId":"89","sever